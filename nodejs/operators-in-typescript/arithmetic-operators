let x: number = 5;
let y: number = 10;

console.log(x + y); // Output: 15
console.log(x - y); // Output: -5
console.log(x * y); // Output: 50
console.log(x / y); // Output: 0.5
console.log(y % x); // Output: 0

let z: number = 3;
z++; 
console.log(z);   // Output: 4

let a: number = 10;
a--;
console.log(a);   // Output: 11
